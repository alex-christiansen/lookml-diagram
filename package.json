{
  "name": "lookml-diagram-extension",
  "sideEffects": [
    "*.css"
  ],
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "start": "webpack serve --no-hot --mode=development",
    "test": "jest",
    "build": "webpack --mode=production",
    "build:ci:clean": "rm -rf ./temp && mkdir ./temp",
    "build:ci": "webpack --mode=production --output-path=./temp",
    "lint": "eslint './src/**/*.ts{,x}' --cache",
    "lint-fix": "eslint './src/**/*.ts{,x}' --fix --cache",
    "analyze": "export ANALYZE_MODE=static && yarn build"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@looker/components": "^1.1.3",
    "@looker/components-test-utils": "^1.1.3",
    "@looker/design-tokens": "^1.1.5",
    "@looker/eslint-config": "^1.1.5",
    "@looker/extension-sdk-react": "^21.0.7",
    "@looker/icons": "1.1.3",
    "@looker/prettier-config": "^1.1.5",
    "@looker/sdk": "^21.0.7",
    "@styled-icons/material": "^10.28.0",
    "@styled-icons/material-outlined": "^10.28.0",
    "@types/d3": "^6.2.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.23",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-test-renderer": "^16.9.2",
    "@testing-library/react": "^11.2.6",
    "@testing-library/jest-dom": "^5.11.6",
    "@types/recharts": "^1.1.21",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@webpack-cli/serve": "^1.3.1",
    "core-js": "^3.11.1",
    "css-loader": "^3.4.2",
    "d3": "^6.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mdx": "^1.6.8",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "jest": "^25.2.4",
    "jest-styled-components": "^7.0.3",
    "prettier": "^1.19.1",
    "prism-react-renderer": "^1.2.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-json-inspector": "^7.1.1",
    "react-query": "^3.13.12",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.1.2",
    "react-test-renderer": "^17.0.1",
    "recharts": "^1.7.1",
    "style-loader": "^1.1.3",
    "styled-components": "^5.2.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^6.2.2",
    "typescript": "^4.2.4",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "eslintIgnore": [
    "src/__tests__/**",
    "src/__mocks__/**",
    "queries.ts",
    "src/components/DiagramFrame/FramePanels/HelpPanelData/**"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "targets": {
            "esmodules": true
          }
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragmaFrag": "React.Fragment"
        }
      ]
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/__mocks__/styleMock.js"
    },
    "transformIgnorePatterns": [
      "/node_modules/?!(react-syntax-highlighter)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/MockData"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/test_setup.js"
    ]
  },
  "prettier": "@looker/prettier-config",
  "prettierConfig": {
    "overrides": {
      "rules": {
        "trailingComma": "all"
      }
    }
  }
}
