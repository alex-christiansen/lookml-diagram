{
  "compilerOptions": {
    "jsx": "react",
    "lib": ["dom", "es2015"],
    "sourceMap": true,
    "strict": true,
    "strictNullChecks": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "skipLibCheck": true,
    "allowJs": true
  },
  "exclude": ["node_modules"]
}
